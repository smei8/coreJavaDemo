package collections;

import java.util.Set;

public class HashSetDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		//features of hashset
			//not indexed
			// does not allow dupliccated values
			// does not maintain the order
		Set<E> myHashSet = new HashSet();
		
		String str1 = "hello";
		String str2 = "Welcome";
		//Room room = new Room(200. 300);
		//wrapper class
		//Integer number = new Integer(10); // new Integer() is deprecated, something old, advice not to use
		Integer number = 10;

		
		myHashSet.add(str1);
		myHashSet.add(str2);
		myHashSet.add(number);
		myHashSet.add(str1);
		
		System.out.println(myHashSet);
		
	}

}
